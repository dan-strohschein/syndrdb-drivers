{
  "name": "@syndrdb/node-driver",
  "version": "0.1.0",
  "description": "Node.js driver for SyndrDB database",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:performance": "jest tests/performance",
    "benchmark": "jest tests/performance/benchmarks.test.ts --runInBand",
    "benchmark:baseline": "npm run benchmark && node scripts/save-baseline.js",
    "benchmark:compare": "jest tests/performance/baseline.test.ts --runInBand",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": ["syndrdb", "database", "driver", "client"],
  "author": "Dan Strohschein",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
