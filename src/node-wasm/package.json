{
  "name": "@syndrdb/node-wasm",
  "version": "2.0.0-alpha.1",
  "description": "SyndrDB Node.js driver powered by Go 1.25 WASM with DWARF v5 debugging support",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "wasm/",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "prebuild": "node scripts/validate-go-version.js && npm run build:wasm",
    "build:wasm": "cd ../golang && ./scripts/build-wasm.sh",
    "build": "tsup && node scripts/copy-wasm.js",
    "dev": "tsup --watch",
    "test": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration --testTimeout=60000",
    "test:performance": "jest tests/performance",
    "test:watch": "jest --watch",
    "benchmark": "jest tests/performance/benchmarks.test.ts --runInBand",
    "benchmark:baseline": "npm run benchmark && node scripts/save-baseline.js",
    "benchmark:compare": "jest tests/performance/baseline.test.ts --runInBand",
    "lint": "eslint src --ext .ts --fix",
    "lint:check": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "docs": "typedoc --out docs-api src/index.ts",
    "generate-types": "ts-node scripts/generate-types.ts",
    "prepublishOnly": "npm run lint:check && npm run type-check && npm test && npm run build"
  },
  "keywords": [
    "syndrdb",
    "database",
    "wasm",
    "webassembly",
    "go",
    "driver",
    "client",
    "migration",
    "schema"
  ],
  "author": "Dan Strohschein",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dan-strohschein/syndrdb-drivers.git",
    "directory": "src/node-wasm"
  },
  "bugs": {
    "url": "https://github.com/dan-strohschein/syndrdb-drivers/issues"
  },
  "homepage": "https://github.com/dan-strohschein/syndrdb-drivers/tree/main/src/node-wasm#readme",
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "typedoc": "^0.25.6",
    "typescript": "^5.3.3"
  },
  "dependencies": {}
}
